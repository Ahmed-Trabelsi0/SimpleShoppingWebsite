{"ast":null,"code":"import productdetails from \"../components/productDetails.vue\";\nexport default {\n  name: \"productDisplay\",\n  components: {\n    productdetails\n  },\n  props: {\n    cart: {\n      type: Array,\n      required: true\n    },\n    primium: {\n      type: Boolean,\n      required: true\n    }\n  },\n  data() {\n    return {\n      product: \"Chaussetes\",\n      description: \"Lorem ipsum dolor sit amet consectetur adipisicing elit. Totam earum fugit voluptate, quasi beatae quaerat quo natus iusto voluptates, incidunt molestias obcaecati. Molestias temporibus aperiam pariatur minima animi repudiandae deserunt possimus repellat voluptates illum labore quae quod velit, minus assumenda voluptatum quisquam ea nostrum.\",\n      details: [\"50% coton\", \"30% laine\", \"20% polyester\"],\n      variants: [{\n        id: 2001,\n        oldPrice: 78,\n        price: 45,\n        color: \"Green\",\n        image: require(\"../assets/socks_green.jpg\"),\n        qty: 20,\n        lastStock: 120\n      }, {\n        id: 2002,\n        oldPrice: 94,\n        price: 50,\n        color: \"Blue\",\n        image: require(\"./../assets/socks_blue.jpg\"),\n        qty: 10,\n        lastStock: 30\n      }],\n      sizes: [\"22-13\", \"15-64\", \"19-94\"],\n      variantIndex: 0\n    };\n  },\n  methods: {\n    changeVariant($event, index) {\n      let variant = this.variants[index];\n      this.image = variant.image;\n      let elements = document.querySelectorAll(\".selected\");\n      elements.forEach(function (element) {\n        element.classList.remove(\"selected\");\n      });\n      event.target.classList.toggle(\"selected\");\n      this.variantIndex = index;\n    },\n    addCart() {\n      if (this.inStock) {\n        this.currentVariant.qty--;\n        this.$emit(\"add-to-cart\", this.currentVariant.id);\n      }\n    },\n    removeCart() {\n      if (this.cart != undefined && this.cart.indexOf(this.currentVariant.id) >= 0) {\n        this.currentVariant.qty++;\n        this.$emit(\"remove-from-cart\", this.currentVariant.id);\n      }\n    }\n  },\n  computed: {\n    currentVariant() {\n      return this.variants[this.variantIndex];\n    },\n    image() {\n      return this.currentVariant.image;\n    },\n    qty() {\n      return this.currentVariant.qty;\n    },\n    stock() {\n      if (this.qty == 1) {\n        return \"Dernier article disponible\";\n      } else if (this.qty <= 10) {\n        return this.qty + \" articles seulement\";\n      }\n      return this.qty + \" articles restants\";\n    },\n    stockColor() {\n      if (this.qty == 1) {\n        return \"#F7DA00\";\n      } else if (this.qty == 0) {\n        return \"#E0144C\";\n      } else if (this.qty <= 10) {\n        return \"#E48900\";\n      }\n      return \"#82CD47\";\n    },\n    inStock() {\n      if (this.qty == 0) {\n        return false;\n      }\n      return true;\n    },\n    price() {\n      return this.currentVariant.price + \" TTC\";\n    },\n    oldprice() {\n      return this.currentVariant.oldPrice + \" TTC\";\n    },\n    shipping() {\n      if (this.primium) {\n        return \"Free Shipping !\";\n      }\n      return \"Shipping fees: 7dt only\";\n    },\n    progresStock() {\n      return parseInt(100 / (this.currentVariant.lastStock / this.currentVariant.qty));\n    },\n    progresStockWidth() {\n      console.log(this.progresStock);\n      return 'width: ' + this.progresStock + '% ;';\n    }\n  }\n};","map":{"version":3,"mappings":"AA4EA,OAAOA,cAAa,MAAO,kCAAkC;AAE7D,eAAe;EACbC,IAAI,EAAE,gBAAgB;EACtBC,UAAU,EAAE;IACVF;EACF,CAAC;EACDG,KAAK,EAAE;IACLC,IAAI,EAAE;MACJC,IAAI,EAAEC,KAAK;MACXC,QAAQ,EAAE;IACZ,CAAC;IACDC,OAAO,EAAE;MACPH,IAAI,EAAEI,OAAO;MACbF,QAAQ,EAAE;IACZ;EACF,CAAC;EACDG,IAAI,GAAG;IACL,OAAO;MACLC,OAAO,EAAE,YAAY;MACrBC,WAAW,EACT,yVAAyV;MAC3VC,OAAO,EAAE,CAAC,WAAW,EAAE,WAAW,EAAE,eAAe,CAAC;MACpDC,QAAQ,EAAE,CACR;QACEC,EAAE,EAAE,IAAI;QACRC,QAAQ,EAAE,EAAE;QACZC,KAAK,EAAE,EAAE;QACTC,KAAK,EAAE,OAAO;QACdC,KAAK,EAAEC,OAAO,