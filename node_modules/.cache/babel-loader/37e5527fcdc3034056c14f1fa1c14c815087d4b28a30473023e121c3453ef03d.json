{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport modalitem from \"./components/modal-item.vue\";\nimport productdisplay from \"./components/productDisplay.vue\";\nexport default {\n  name: \"App\",\n  components: {\n    productdisplay,\n    modalitem\n  },\n  data() {\n    return {\n      cart: [],\n      primium: true,\n      total: 0\n    };\n  },\n  methods: {\n    updateCart(id) {\n      //this.cart++;\n      this.cart.push(id);\n    },\n    removeCart(id) {\n      //this.cart--;\n      let index = this.cart.indexOf(id);\n      if (index >= 0) {\n        this.cart.splice(index, 1);\n      }\n    }\n  },\n  computed: {\n    cartItemsNumber() {\n      return this.cart.length;\n    },\n    totalCart() {\n      return this.total + \" Dt\";\n    }\n  }\n};","map":{"version":3,"mappings":";AA0BA,OAAOA,SAAQ,MAAO,6BAA6B;AACnD,OAAOC,cAAa,MAAO,iCAAiC;AAE5D,eAAe;EACbC,IAAI,EAAE,KAAK;EACXC,UAAU,EAAE;IACVF,cAAc;IACdD;EACF,CAAC;EACDI,IAAI,GAAG;IACL,OAAO;MACLC,IAAI,EAAE,EAAE;MACRC,OAAO,EAAE,IAAI;MACbC,KAAK,EAAE;IACT,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACPC,UAAU,CAACC,EAAE,EAAE;MACb;MACA,IAAI,CAACL,IAAI,CAACM,IAAI,CAACD,EAAE,CAAC;IACpB,CAAC;IACDE,UAAU,CAACF,EAAE,EAAE;MACb;MACA,IAAIG,KAAI,GAAI,IAAI,CAACR,IAAI,CAACS,OAAO,CAACJ,EAAE,CAAC;MACjC,IAAIG,KAAI,IAAK,CAAC,EAAE;QACd,IAAI,CAACR,IAAI,CAACU,MAAM,CAACF,KAAK,EAAE,CAAC,CAAC;MAC5B;IACF;EACF,CAAC;EACDG,QAAQ,EAAE;IACRC,eAAe,GAAG;MAChB,OAAO,IAAI,CAACZ,IAAI,CAACa,MAAM;IACzB,CAAC;IACDC,SAAS,GAAG;MACV,OAAO,IAAI,CAACZ,KAAI,GAAI,KAAK;IAC3B;EACF;AACF,CAAC","names":["modalitem","productdisplay","name","components","data","cart","primium","total","methods","updateCart","id","push","removeCart","index","indexOf","splice","computed","cartItemsNumber","length","totalCart"],"sourceRoot":"","sources":["C:\\Users\\Ahmed Trabelsi\\Desktop\\vueapp\\src\\App.vue"],"sourcesContent":["<template>\n  <div id=\"app\">\n    <div class=\"container mt-4\">\n      <div class=\"nav-bar d-flex justify-content-between px-5 align-items-center\">\n        <div class=\"container\">\n          <img src=\"../src/assets/logo.png\" class=\"logo\" alt=\"vue_logo\" >\n        </div>\n        <div class=\"cart d-flex justify-content-between\">\n          <div class=\"cartLogo\">\n            <h2><i class=\"bi bi-cart3 text-dark\"></i></h2>\n          </div>\n          <div>\n            <span class=\"cartNumber\">{{cartItemsNumber}}</span>\n          </div>\n          <div class=\"container totalCart d-flex justify\">\n            <span class=\"text-light\">{{ totalCart }}</span>\n          </div>\n        </div>\n      </div>\n    </div>\n  </div>\n  <productdisplay @add-to-cart=\"updateCart\" @remove-from-cart=\"removeCart\" :cart=\"cart\" :primium=\"primium\" />\n  <modalitem />\n</template>\n\n<script>\nimport modalitem from \"./components/modal-item.vue\";\nimport productdisplay from \"./components/productDisplay.vue\";\n\nexport default {\n  name: \"App\",\n  components: {\n    productdisplay,\n    modalitem,\n  },\n  data() {\n    return {\n      cart: [],\n      primium: true,\n      total: 0\n    };\n  },\n  methods: {\n    updateCart(id) {\n      //this.cart++;\n      this.cart.push(id);\n    },\n    removeCart(id) {\n      //this.cart--;\n      let index = this.cart.indexOf(id);\n      if (index >= 0) {\n        this.cart.splice(index, 1);\n      }\n    },\n  },\n  computed: {\n    cartItemsNumber() {\n      return this.cart.length;\n    },\n    totalCart() {\n      return this.total + \" Dt\";\n    }\n  }\n};\n</script>\n\n<style>\nbody {\n  background-color: #eeeeee;\n  margin: 0px;\n  font-family: tahoma;\n  color: #282828;\n}\n\n.nav-bar {\n  background: linear-gradient(-90deg, #84cf6a, #16c0b0);\n  box-shadow: 5px 5px 19px #8f8f8f,\n    -5px -5px 19px #ffffff;\n  height: 60px;\n  border-radius: 10px;\n  margin-bottom: 9px;\n}\n\n.logo{\n  max-width: 40px;\n    filter: drop-shadow(-1px 1px 0 rgb(255, 255, 255, 0.7)) drop-shadow(-1px -1px 0 rgb(255, 255, 255, 0.5));\n}\n.cartNumber{\n  background-color: #098EDB;\n  border-radius: 10px;\n  color: #e0e0e0;\n  padding: 1px 6px 1px 6px;\n}\n\n.cart{\n  cursor: pointer;\n}\n\n.totalCart{\n  min-width: 60px;\n}\n</style>\n"]},"metadata":{},"sourceType":"module","externalDependencies":[]}