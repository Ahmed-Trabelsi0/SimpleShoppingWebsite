{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport modalitem from \"./components/modal-item.vue\";\nimport productdisplay from \"./components/productDisplay.vue\";\nimport signUp from \"@component\";\nexport default {\n  name: \"App\",\n  components: {\n    productdisplay,\n    modalitem\n  },\n  data() {\n    return {\n      cart: [],\n      primium: true,\n      total: 0,\n      showModal: false\n    };\n  },\n  methods: {\n    updateCart(currentVariant) {\n      //this.cart++;\n      this.cart.push(currentVariant);\n      this.total += currentVariant.price;\n    },\n    removeCart(currentVariant) {\n      //this.cart--;\n      let index = this.cart.indexOf(currentVariant);\n      if (index >= 0) {\n        this.cart.splice(index, 1);\n        this.total -= currentVariant.price;\n        if (this.total < 0) {\n          this.total = 0;\n        }\n      }\n    },\n    toggleModal() {\n      this.showModal = !this.showModal;\n    }\n  },\n  computed: {\n    cartItemsNumber() {\n      return this.cart.length;\n    },\n    totalCart() {\n      return this.total + \" DT\";\n    }\n  }\n};","map":{"version":3,"mappings":";AAiCA,OAAOA,SAAQ,MAAO,6BAA6B;AACnD,OAAOC,cAAa,MAAO,iCAAiC;AAC5D,OAAOC,MAAK,MAAO,YAAY;AAC/B,eAAe;EACbC,IAAI,EAAE,KAAK;EACXC,UAAU,EAAE;IACVH,cAAc;IACdD;EACF,CAAC;EACDK,IAAI,GAAG;IACL,OAAO;MACLC,IAAI,EAAE,EAAE;MACRC,OAAO,EAAE,IAAI;MACbC,KAAK,EAAE,CAAC;MACRC,SAAS,EAAE;IACb,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACPC,UAAU,CAACC,cAAc,EAAE;MACzB;MACA,IAAI,CAACN,IAAI,CAACO,IAAI,CAACD,cAAc,CAAC;MAC9B,IAAI,CAACJ,KAAI,IAAKI,cAAc,CAACE,KAAK;IACpC,CAAC;IACDC,UAAU,CAACH,cAAc,EAAE;MACzB;MACA,IAAII,KAAI,GAAI,IAAI,CAACV,IAAI,CAACW,OAAO,CAACL,cAAc,CAAC;MAC7C,IAAII,KAAI,IAAK,CAAC,EAAE;QACd,IAAI,CAACV,IAAI,CAACY,MAAM,CAACF,KAAK,EAAE,CAAC,CAAC;QAC1B,IAAI,CAACR,KAAI,IAAKI,cAAc,CAACE,KAAK;QAClC,IAAI,IAAI,CAACN,KAAI,GAAI,CAAC,EAAE;UAClB,IAAI,CAACA,KAAI,GAAI,CAAC;QAChB;MACF;IACF,CAAC;IACDW,WAAW,GAAG;MACZ,IAAI,CAACV,SAAQ,GAAI,CAAC,IAAI,CAACA,SAAS;IAClC;EACF,CAAC;EACDW,QAAQ,EAAE;IACRC,eAAe,GAAG;MAChB,OAAO,IAAI,CAACf,IAAI,CAACgB,MAAM;IACzB,CAAC;IACDC,SAAS,GAAG;MACV,OAAO,IAAI,CAACf,KAAI,GAAI,KAAK;IAC3B;EACF;AACF,CAAC","names":["modalitem","productdisplay","signUp","name","components","data","cart","primium","total","showModal","methods","updateCart","currentVariant","push","price","removeCart","index","indexOf","splice","toggleModal","computed","cartItemsNumber","length","totalCart"],"sourceRoot":"","sources":["C:\\Users\\Ahmed Trabelsi\\Desktop\\Education\\Practice\\VueJs\\vueapp\\src\\App.vue"],"sourcesContent":["<template>\n  <div id=\"app\">\n    <div class=\"container mt-4\">\n      <div class=\"nav-bar d-flex justify-content-between px-5 align-items-center\">\n        <div class=\"container\">\n          <img src=\"../src/assets/logo.png\" class=\"logo\" alt=\"vue_logo\">\n        </div>\n        <div class=\"cart d-flex\">\n          <div class=\"total-cart d-flex justify-content-center align-items-center me-1\">\n            <span class=\"text-light totalCart\" @click=\"toggleModal\">{{ 'Total: ' + totalCart }}</span>\n          </div>\n          <div class=\"d-flex ps-5 border-start\">\n            <div class=\"cartLogo\" @click=\"toggleModal\">\n              <h2><i class=\"bi bi-cart3 text-dark\"></i></h2>\n            </div>\n            <div @click=\"toggleModal\">\n              <span class=\"cartNumber\">{{ cartItemsNumber }}</span>\n            </div>\n          </div>\n        </div>\n      </div>\n    </div>\n  </div>\n  <productdisplay @add-to-cart=\"updateCart\" @remove-from-cart=\"removeCart\" :cart=\"cart\" :primium=\"primium\"\n    :totalCart=\"total\" />\n  <div>\n    <modalitem @deleteProduct=\"removeCart\" title='Cart' v-if=\"showModal\" @closeModal=\"toggleModal\" :cart=\"cart\"\n      :totalCart=\"totalCart\" />\n  </div>\n  \n</template>\n\n<script>\nimport modalitem from \"./components/modal-item.vue\";\nimport productdisplay from \"./components/productDisplay.vue\";\nimport signUp from \"@component\";\nexport default {\n  name: \"App\",\n  components: {\n    productdisplay,\n    modalitem,\n  },\n  data() {\n    return {\n      cart: [],\n      primium: true,\n      total: 0,\n      showModal: false\n    };\n  },\n  methods: {\n    updateCart(currentVariant) {\n      //this.cart++;\n      this.cart.push(currentVariant);\n      this.total += currentVariant.price;\n    },\n    removeCart(currentVariant) {\n      //this.cart--;\n      let index = this.cart.indexOf(currentVariant);\n      if (index >= 0) {\n        this.cart.splice(index, 1);\n        this.total -= currentVariant.price;\n        if (this.total < 0) {\n          this.total = 0;\n        }\n      }\n    },\n    toggleModal() {\n      this.showModal = !this.showModal;\n    },\n  },\n  computed: {\n    cartItemsNumber() {\n      return this.cart.length;\n    },\n    totalCart() {\n      return this.total + \" DT\";\n    }\n  }\n};\n</script>\n\n<style>\nbody {\n  background-color: #eeeeee;\n  margin: 0px;\n  color: #282828;\n}\n\n.nav-bar {\n  background: linear-gradient(-90deg, #84cf6a, #16c0b0);\n  box-shadow: 5px 5px 19px #8f8f8f,\n    -5px -5px 19px #ffffff;\n  height: 60px;\n  border-radius: 10px;\n}\n\n.logo {\n  max-width: 40px;\n  filter: drop-shadow(-1px 1px 0 rgb(255, 255, 255, 0.7)) drop-shadow(-1px -1px 0 rgb(255, 255, 255, 0.5));\n}\n\n.cartNumber {\n  background-color: #098EDB;\n  border-radius: 20px;\n  color: #e0e0e0;\n  padding: 1px 7px 2px 7px;\n}\n\n.totalCart,\n.cartLogo,\n.cartNumber {\n  cursor: pointer;\n}\n\n.total-cart {\n  min-width: 100px;\n}\n</style>\n"]},"metadata":{},"sourceType":"module","externalDependencies":[]}